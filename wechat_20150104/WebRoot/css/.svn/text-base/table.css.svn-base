@CHARSET "UTF-8";
table{
	font-family: "Helvetica Neue","Hiragino Sans GB","Microsoft YaHei","微软雅黑","黑体",Arial,sans-serif;
	margin: 1% 1%;
	width: 98%;
	border-collapse: collapse;
	background-color: white;
}

tr{
	background-color:expression((this.sectionRowIndex==0)?"":(this.sectionRowIndex%2==0)?"red":"blue")
}

th{
	text-align: center;
	font-size: 16px;
}

td.title{
	text-align: center;
	font-size: 16px;
	width: 10%;
}

td.content{
	font-size: 14px;
}
