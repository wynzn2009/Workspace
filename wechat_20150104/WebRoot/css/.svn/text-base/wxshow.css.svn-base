.wxshow{
	font-size: 16px;
	line-height: 20px;
	font-family: "宋体";
	padding: 20px;
}
.wxshow h2{
	text-align: center;
	border: 0 none;
	background-color: transparent;
	padding: 0;
	margin: 10px 0;
	font-size: 24px;
}
.wxshow h3{
	text-align: center;
	border: 0 none;
	background-color: transparent;
	padding: 0;
	margin: 6px 0;
	font-size: 14px;
	font-weight: normal;
	color: #5a5a5a;
}
.wxshow img{
	max-width: 95%;
}
.wxshow p{
	margin: 0;
}

.tdt.tdl{
	font-size: 16px;
}